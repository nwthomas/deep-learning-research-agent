LANGSMITH_PROJECT=deep-learning-research-agent

# Tavily api key for web search
TAVILY_API_KEY=your_tavily_api_key_here

# Supervisor model variables
SUPERVISOR_MODEL_API_KEY=your_model_api_key_here
SUPERVISOR_MODEL_BASE_URL=your_model_base_url_here
SUPERVISOR_MODEL_NAME=your_model_name_here
